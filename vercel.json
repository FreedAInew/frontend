{
  "rewrites": [
    { "source": "/chat", "destination": "/pages/chat.html" },
    { "source": "/login", "destination": "/pages/login.html" },
    { "source": "/register", "destination": "/pages/register.html" },
    { "source": "/complete-profile", "destination": "/pages/complete-profile.html" },
    { "source": "/forgot-password", "destination": "/pages/forgot-password.html" },

    { "source": "/js/(.*)", "destination": "/assets/js/$1" },
    { "source": "/css/(.*)", "destination": "/assets/css/$1" },

    { "source": "/", "destination": "/index.html" }
  ]
}
